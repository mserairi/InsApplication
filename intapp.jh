
entity Enfant {
	nom String required
    prenom String required
    age Integer
    
}
entity Category {
   libile String required
   description String required
   tarif Float
}

relationship ManyToOne
{
Category{sousCat(libile)} to Category

}

relationship ManyToMany
{
Enfant{Suivre(libile)} to Category{Inscrits}

}
relationship ManyToMany
{
Enfant{Parent(login)} to User

}
